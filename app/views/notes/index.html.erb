<%= link_to 'Home', notes_path%>
<%= link_to 'New', new_note_path%>

<h1 style="color:gray;">Notes</h1>
<%= link_to "Create a new note", new_note_path%>
<% @notes.each do |note| %>
  <h2 style="color:gray;"><%= note.title%></h2>

  <%= link_to 'Show Note', note_path(note)%>
  <%= link_to 'Edit Note', edit_note_path(note)%>
  <%= link_to 'Delete Note', note_path(note), method: :delete%>
<% end %>